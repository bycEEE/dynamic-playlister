<%= form_tag(songs_path, method: "post") do %>
  <%= hidden_field_tag :user_id, current_user.id %>
  <%= hidden_field_tag :playlist_id, params["id"] %>
  <%= label_tag "Enter Youtube URL or Search Term: " %>
  <%= text_field_tag :search_term_or_url %>
  <%= submit_tag "Add Song", :id => "intelligent-add-song"%>
<% end %>